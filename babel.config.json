{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "babel-plugin-rewire",
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "alias": {
                    "@": "./" // Adjust this based on your alias configuration
                }
            }
        ]
    ]
}